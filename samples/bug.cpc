#include <stdio.h>
long double glob = 8;
cps void set_glob() { glob = 16; }
cps long double identity(long double x) { return x; }
cps void h(int x, long double y, long double z) { printf("%d %d %d ", x, (int) y, (int) z);return;}
cps int foo(int r){
    long double x = 100;
    set_glob();
    x = identity(glob);
    h(r, x+1, x);
    return 33;
}
int main(){
    cpc_spawn {printf("%d\n",foo(0));}
    cpc_main_loop();
    return 0;
}
