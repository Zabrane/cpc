#include <stdio.h>

cps int f(int x) { return x; }

cps void
cpsmain()
{
    int i, j;
    volatile int s;
    for(i = 0; i < 10000; i++) {
        s = 0;
        for(j = 0; j < 1000; j++)
            s += f(j);
    }
    printf("%d\n", s);
}

int
main()
{
    cpc_spawn cpsmain();
    cpc_main_loop();
    return 0;
}
